{
  "name": "whitelabel-pattern-library",
  "version": "0.1.0",
  "description": "All React components used in whitelabel projects.",
  "main": "index.js",
  "private": true,
  "dependencies": {
  },
  "scripts": {
    "start": "npx styleguidist server",
    "build": "npx styleguidist build",
    "jest": "jest",
    "visual": "npx styleguidist build && styleguidist-visual test --url \"file://$(pwd)/styleguide/index.html\" --wait 150 --threshold 0.1",
    "approve": "npx styleguidist-visual approve",
    "create": "./scripts/create.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "jest": "^24.0.0",
    "react-styleguidist": "^8.0.6",
    "react-styleguidist-visual": "github:theZieger/react-styleguidist-visual#use-elementHandle-screenshot",
    "react-testing-library": "^5.4.4"
  }
}
